Config { font = "xft:MonoSpace:pixelsize=13:antialias=true"
       , borderColor = "black"
       , border = TopB
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top L 225
       , lowerOnStart = True
       , persistent = False
       , hideOnStart = False
       , commands = [ Run Network "enp0s25" ["-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Network "wlp3s0" ["-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%", "-L", "30", "-H", "70", "--normal", "green", "--high", "red"] 10
                    , Run Swap [] 10
                    , Run Com "ip" ["route"] "" 10
                    , Run BatteryP ["BAT0"] [ "-t" , "<acstatus>" , "-L" , "20" , "-H" , "80" , "--low" , "red" , "--high" , "green" , "--" , "-o" , "Battery: <left>% (<timeleft>)" , "-O" , "<fc=orange>Charging</fc>: <left>% (<timeleft>)" , "-i" , "<fc=yellow>Idle</fc>: <left>% (<timeleft>)" ] 10
                    , Run Date "%a %m/%d %H:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader%  }{ %cpu% | %memory% * %swap% | %enp0s25%%wlp3s0% | <fc=#ee9a00>%date%</fc> | %battery%   "
       }
